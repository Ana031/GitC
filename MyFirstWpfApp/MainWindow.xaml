        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyFirstWpfApp"
        xmlns:y="clr-namespace:MyFirstWpfApp.Views"
        mc:Ignorable="d"
        Title="Lanches" Height="450" Width="800">
    <Grid>
        <y:ucLogin x:Name="FormLogin" loginCorrect="FormLogin_loginCorrect" ></y:ucLogin>
